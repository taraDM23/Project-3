  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "axios": "^0.18.1",
    "bootstrap": "^4.4.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.6",
    "jsonwebtoken": "^8.5.1"
  }
}
